{% macro question(quest) %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{ quest.title }}</h3>
        </div>
        <div class="panel-body">
            {{ quest.body }}
        </div>
        <div class="panel-footer">Panel footer</div>
    </div>
{% endmacro %}
